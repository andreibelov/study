CREATE TABLE users (ID BIGINT NOT NULL, BIRTHDAY DATETIME, EMAIL VARCHAR(255), FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), LOGIN VARCHAR(255), PASSWORD VARCHAR(255), PHONE VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE messages (ID BIGINT NOT NULL, TEXT VARCHAR(255), OWNER_ID BIGINT, PRIMARY KEY (ID))
ALTER TABLE messages ADD CONSTRAINT FK_messages_OWNER_ID FOREIGN KEY (OWNER_ID) REFERENCES users (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
